<html>
<head>
    <style type="text/css">
        table, tr, td {
            border-collapse: collapse;
            margin: 0;
            font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
            font-size: 14px;
        }

        th {
            background: #f2f2f2;
        }

        .font-size-18 {
            font-size: 18px;
        }

        .font-size-20 {
            font-size: 20px;
            font-weight: bold;
        }

        .border td, th {
            border-right: 1px solid black;
            border-bottom: 1px solid black;
            border-collapse: collapse;
        }

        tr.no-border > td {
            border-right: 0 none;
            border-bottom: 0 none;
        }

        .leftPadding td, th {
            padding: 4px 4px 4px 10px;
        }

        .no-right-border tr td:nth-last-child(1), th:nth-last-child(1) {
            border-right: 0 none;
        }

        .fixed-font-color {
            color: #585858;
        }

        .highlighted_col {
            border-top: 2px solid #999;
            border-bottom: 2px solid #999;
            padding-top: 8px;
            padding-bottom: 8px;
        }

        .r_align {
            text-align: right;
        }

        .sep_section {
            padding-top: 20px;
            padding-bottom: 6px;
        }

        .spacer {
            height: 20px
        }

        .tag {
            padding: 5px 18px;
            border: 1px solid #aaa;
            border-radius: 6px;
            margin-right: 5px;
        }
    </style>
</head>

<body>
<table width="100%" class="border" cellspacing="0" cellpadding="0"
style="border: 1px solid #000">
    <tr class="leftPadding">
        <td class="font-size-18" align="center">Vulcan Express Pvt Ltd.</td>
    </tr>
    <tr class="leftPadding">
        <td align="center" class="font-size-18">INTER_FACILITY Trip Sheet</td>
    </tr>
    <tr class="no-border">
        <td>
            <table width="100%" cellspacing="0" cellpadding="0">
                <tr class="no-border">
                    <td>
                        <table width="100%" height="180" class="border
leftPadding" cellspacing="0" cellpadding="0">
                            <tr>
                                <td>Trip Start Date-time:</td>

<td>#if(${dateutils.dateToString($trip.startTime,
                                    'dd-MMM-yyyy
hh:mm:ss')})${dateutils.dateToString($trip.startTime,
                                    'dd-MMM-yyyy hh:mm:ss')} #end</td>
                            </tr>
                            <tr>
                                <td>Origin Name</td>

<td>#if($sourceFacility.name)<![CDATA[$sourceFacility.name]]>#end</td>
                            </tr>
                            <tr>
                                <td>Origin Code</td>
                                <td
class="font-size-20">#if($sourceFacility.code)
                                    <![CDATA[$sourceFacility.code]]>#end</td>
                            </tr>
                            <tr>
                                <td>Origin Address:</td>

<td>#if($sourceFacility.address)<![CDATA[$sourceFacility.address]]>#end</td>
                            </tr>
                        </table>
                    </td>
                    <td>
                        <table width="100%" height="180" class="border
leftPadding" cellspacing="0" cellpadding="0">
                            <tr>
                                <td><img id="barcode_trip"></img><br>$trip.code
                                </td>
                            </tr>
                            <tr>
                                <td><img
id="barcode_docket"></img><br>$trip.coloaderDocketNumber
                                </td>
                            </tr>
                        </table>
                    </td>
                    <td>
                        <table width="100%" height="180" class="border
leftPadding no-right-border" cellspacing="0"
                               cellpadding="0">
                            <tr>
                                <td>Co-Loader/ Transporter Name:</td>
                                <td>#if($trip.vehicle.owner.type==

'COLOADER')#if($coloader)$coloader.name#else$trip.vehicle.owner.ownerId#end#elseif($trip.vehicle.owner.type==

'TENANT')$trip.vehicle.owner.ownerId#elseif($trip.vehicle.owner.type==
                                    'RIDER')$trip.rider.code
($trip.rider.name)#end</td>
                            </tr>
                            <tr>
                                <td>Destination Name</td>

<td>#if($destinationFacility.name)<![CDATA[$destinationFacility.name]]>#end</td>
                            </tr>
                            <tr>
                                <td>Destination Code</td>
                                <td
class="font-size-20">#if($destinationFacility.code)

<![CDATA[$destinationFacility.code]]>#end</td>
                            </tr>
                            <tr>
                                <td>Destination Address:</td>

<td>#if($destinationFacility.address)<![CDATA[$destinationFacility.address]]>#end</td>
                            </tr>
                        </table>
                    </td>
                </tr>
            </table>
        </td>
    </tr>

    <tr class="no-border">
        <td>
            #if ($trip.type == 'INTER_FACILITY')
                <table width="100%" class="border leftPadding
no-right-border" cellspacing="0" cellpadding="0">
                    <tr>
                        <th>Sr No</th>
                        <th>Bag/ Shipmnet ID</th>
                        <th>No. of shipments</th>
                        <th>No of Parts (Mulit part)</th>
                        <th>Invoice Amount</th>
                        <th>Bag Dead Wt (Kgs)</th>
                    </tr>
                    #set ($totalCOD = 0)
                    #foreach($task in $tasks)
                        #if($task.type.equals('HUB_TO_HUB_DELIVERY'))
                            <tr style="font-size: 13px;">
                                <td class="border">${foreach.count}</td>
                                <td
class="border">#if($task.consignment.code)$task.consignment.code#end

<br/>#if($task.consignment.type)$task.consignment.type#end</td>

                                #set ($noOfItems = 1)
                                #if ($task.consignment.type == 'BATCH')
                                    #if ($taskToBatch &&
$taskToBatch[$task.consignment.code] &&
$taskToBatch[$task.consignment.code].batchItems &&
$taskToBatch[$task.consignment.code].batchItems.size() > 0)
                                        #set ($noOfItems =
$taskToBatch[$task.id].batchItems.size())
                                    #end
                                #end
                                <td class="border">
                                    #if($task.consignment.shipmentItems)
                                     $task.consignment.shipmentItems.size()
                                #elseif($task.consignment.type == 'BATCH')
                                        $noOfItems
                                    #end
                                </td>
                                #set ($noOfParts = 1)
                                <td class="border">
                                    #if($task.consignment.shipmentParts)
                                     $task.consignment.shipmentParts.size()
                                #elseif($task.consignment.type == 'BATCH')
                                        $noOfParts
                                    #end
                                </td>
                                #set ($invoiceAmount = 0)
                                #if($task.consignment.type == 'BATCH'
&& $taskToBatch && $taskToBatch[$task.consignment.code] &&
$taskToBatch[$task.consignment.code].batchItems)
                                    #foreach($batchItem in
$taskToBatch[$task.consignment.code].batchItems)

#if($taskToShipment.get($batchItem.consignmentCode))
                                            #foreach($shipment in
$taskToShipment.get($batchItem.consignmentCode))
                                                #foreach($item in
$shipment.shipmentItems)
                                                    #set
($invoiceAmount = $invoiceAmount + $item.invoiceAmount)
                                                #end
                                            #end
                                        #end
                                    #end
                                #else

#if($taskToShipment.get($task.consignment.code))
                                        #foreach($shipment in
$taskToShipment.get($task.consignment.code))
                                            #foreach($item in
$shipment.shipmentItems)
                                                #set ($invoiceAmount =
$invoiceAmount + $item.invoiceAmount)
                                            #end
                                        #end
                                    #end
                                #end
                                <td class="border">
                                    $invoiceAmount
                                </td>
                                #set ($bagWeight = 0.00)
                                #if ($task.consignment.type == 'BATCH')
                                    #if ($taskToBatch &&
$taskToBatch[$task.consignment.code] &&
$taskToBatch[$task.consignment.code].weightInGram > 0)

#set($weightInGram=$numberutils.newBigDecimal($taskToBatch[$task.consignment.code].weightInGram))

#set($divisor=$numberutils.newBigDecimal(1000))
                                        #set($bagWeight =
$numberutils.divide($weightInGram,$divisor,3))
                                    #end
                                #end
                                <td class="border">
                                    $bagWeight
                                </td>
                            </tr>
                        #end
                    #end
                </table>
            #else
                <table width="100%" class="border" style="font-size:14px;">
                    <tr style="background: #F1F1F1;font-weight:
bold;text-align: center;" class="fixed-font-color">
                        <td class="border" width="6%">Sr.No</td>
                        <td class="border">AWB Detail</td>
                        <td class="border">Customer Detail</td>
                        <td class="border" width="15%">COD Amount</td>
                        <td class="border">Receiver Name, Sign &amp; Phone</td>
                        <td class="border">Remarks</td>
                    </tr>
                    #set ($totalCOD = 0)
                    #foreach($task in $tasks)

                        #if($task.type.equals('CUSTOMER_DELIVERY') or
$task.type.equals(
                            'CUSTOMER_PICKUP') or $task.type.equals(
                            'SELLER_PICKUP_SHIPMENT_HANDOVER') or
$task.type.equals(
                            'EXCHANGE') or
$task.type.equals('EXCHANGE_HANDOVER'))
                            <tr style="font-size: 13px;">
                                <td class="border">${foreach.count}</td>
                                <td
class="border">#if($task.consignment.code)$task.consignment.code#end

<br/>#if($task.consignment.handoverMode)$task.consignment.handoverMode#end
                                    <br/>#if($task.type == 'EXCHANGE')
                                        <strong>$task.type</strong><br/>
                                        #foreach($shipment in
$task.consignment.collectibleShipments)
                                            #foreach($item in
$shipment.shipmentItems)
                                                $item.identifier<br/>
                                            #end
                                        #end
                                    #end</td>
                                <td class="border">

<![CDATA[#if($task.location.name)$task.location.name#end$task.location.addressLine1]]>

<br/>#if($task.location.phone)$task.location.phone#end

#if($taskToShipment.get($task.consignment.code))
                                        #set ($shipment =
$taskToShipment.get($task.consignment.code))

#if($shipment.alternateDestinationAddress.addressLine1)
                                            <br/>
                                            Alternate
                                            Address:

<![CDATA[#if($shipment.alternateDestinationAddress.name)$shipment.alternateDestinationAddress.name
                                            ,#end
$shipment.alternateDestinationAddress.addressLine1]]>

<br/>#if($shipment.alternateDestinationAddress.phone)
$shipment.alternateDestinationAddress.phone#end
                                        #end
                                    #end
                                </td>
                                <td class="border" align="center">
                                    #if($task.expectedTransactionAmount)
                                $task.expectedTransactionAmount
                                #set ($totalCOD = $totalCOD +
$task.expectedTransactionAmount)
                            #end
                                </td>
                                #set ($imageUrl = '')
                                #if ($task.taskActionResult &&
$task.taskActionResult.actionsPerformed &&
$task.taskActionResult.actionsPerformed.size() > 0)
                                    #foreach($action in
$task.taskActionResult.actionsPerformed)
                                        #if ($action.actionCode ==
'CAPTURE_DIGITAL_SIGNATURE')
                                            #set ($imageUrl = $action.output)
                                        #end
                                    #end
                                #end
                                #if ($imageUrl == '')
                                    <td class="border"></td>
                                #else
                                    <td class="border">
                                        <img class="barcode" src="$imageUrl"/>
                                    </td>
                                #end
                                <td class="border"></td>
                            </tr>
                        #end
                    #end
                    <tr style="background: #F1F1F1;font-weight:
bold;text-align: center;" class="fixed-font-color">
                        <td class="border"></td>
                        <td class="border"></td>
                        <td class="border">Total COD Amount:</td>
                        <td class="border">$totalCOD</td>
                        <td class="border"></td>
                        <td class="border"></td>
                    </tr>
                </table>
            #end
        </td>
    </tr>

    <tr>
        <td>
            <table width="100%" class="leftPadding" cellspacing="0"
cellpadding="0">
                <tr class="no-border">
                    <td colspan="3" height="50"></td>
                </tr>
                <tr class="no-border">
                    <td></td>
                    <td width="150">Vehicle Number:</td>
                    <td
width="150">#if($trip.vehicle.registrationNo)$trip.vehicle.registrationNo#end</td>
                </tr>
                <tr class="no-border">
                    <td>Created by:</td>
                    <td width="150">Driver Name:</td>
                    <td
width="150">#if($trip.rider.name)$trip.rider.name#end</td>
                </tr>
                <tr class="no-border">
                    <td colspan="3">Note:-This is system generated
Manifest,does not required any stamp or signature
                    </td>
                </tr>
            </table>
        </td>
    </tr>
    <tr class="leftPadding">
        <td>Regd Office: 61/1, Block-lll Kirti Nagar, WHS, New Delhi-110015</td>
    </tr>
</table>
</body>
</html>

